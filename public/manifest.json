{
  "name": "Autostaker for Terra",
  "description": "Autostake your rewards on the mirror(terra) network",
  "manifest_version": 2,
  "key": "",
  "version": "1.0.0",
  "permissions": [ "storage", "activeTab" ],
  "icons": {
     "128": "icon-128.png",
     "16": "icon-32.png",
     "48": "icon-32.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icon-32.png",
      "48": "icon-32.png"
    },
    "default_popup": "popup.html",
    "default_title": "Autostaker"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}